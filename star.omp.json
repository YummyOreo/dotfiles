{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "#cc241d",
                    "style": "plain",
                    "template": "root <#ebdbb2>in</> ",
                    "type": "root"
                },
                {
                    "foreground": "#fe8019",
                    "properties": {
                        "style": "folder"
                    },
                    "style": "plain",
                    "template": "{{ .Path }} ",
                    "type": "path"
                },
                {
                    "foreground": "#d3869b",
                    "foreground_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#d3869b{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#d3869b{{ end }}",
                        "{{ if gt .Ahead 0 }}#b16286{{ end }}",
                        "{{ if gt .Behind 0 }}#b16286{{ end }}"
                    ],
                    "properties": {
                        "branch_icon": "\ue725 ",
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "powerline",
                    "template": "<#ebdbb2>on</> {{ .HEAD }}",
                    "type": "git"
                },
                {
                    "foreground": "#689d6a",
                    "properties": {
                        "fetch_version": true
                    },
                    "style": "plain",
                    "template": "<#ebdbb2>via</> \ue781 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
                    "type": "node"
                },
                {
                    "type": "python",
                    "style": "powerline",
                    "foreground": "#478788",
                    "template": " \uE235 {{ .Full }} "
                },
                {
                    "type": "rust",
                    "style": "powerline",
                    "foreground": "#d65d0e",
                    "template": " <#ebdbb2>via</> ðŸ¦€ {{ .Full }} "
                },
                {
                    "type": "executiontime",
                    "style": "powerline",
                    "foreground": "#7c6f64",
                    "template": " <#7c6f64>\ufbab</> {{ .FormattedMs }} ",
                    "properties": {
                        "threshold": 500,
                        "style": "austin"
                    }
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#d65d0e",
                    "style": "plain",
                    "template": "\u279c ",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "version": 2
}
